generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  googleId  String   @unique
  email     String   @unique
  name      String
  image     String?
  createdAt DateTime @default(now())
}
